{% extends 'partials/base.html.twig' %}

{% block content %}

  <h1>{{ page.title }}</h1>
  <a href="{{ issue_base_url }}">Back to issue list</a>

  <table>
    <tr>
      <th>Nid</th>
      <th>Component</th>
      <th>Status</th>
    </tr>
    <tr>
      <td>{{ nid }}</td>
      <td>{{ component }}</td>
      <td>{{ status }}</td>
  </table>

  {{ content }}
{% endblock %}
